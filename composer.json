{
    "name": "typo3/cms-base-distribution",
    "description": "TYPO3 CMS Base Distribution",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://ueberland-tecklenburg.de",
    "keywords": [
        "TYPO3 CMS"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "audit": {
            "block-insecure": false
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
	"balumedien/ueberland": "^1.0",
	"christianknell/sp_ueberland": "^1.0",
        "cobweb/flush_language_cache": "^4.0",
        "georgringer/news": "^13.0",
        "gridelementsteam/gridelements": "^12.0",
        "helhum/typo3-console": "^8.3",
        "internetgalerie/ig-slug": "^1.0",
        "jambagecom/static-info-tables-de": "^12.4",
        "josefglatz/beuser-fastswitch": "^5.0",
        "laxap/bootstrap-grids": "^6.0",
        "mask/mask": "^8.3",
        "paulbeck/tgm-copyright": "^3.3",
        "sjbr/static-info-tables": "^12.4",
        "typo3/cms-about": "*",
        "typo3/cms-adminpanel": "^12.4",
        "typo3/cms-backend": "^12.4",
        "typo3/cms-belog": "^12.4",
        "typo3/cms-beuser": "^12.4",
        "typo3/cms-core": "^12.4",
        "typo3/cms-dashboard": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-extensionmanager": "^12.4",
        "typo3/cms-felogin": "^12.4",
        "typo3/cms-filelist": "^12.4",
        "typo3/cms-filemetadata": "^12.4",
        "typo3/cms-fluid": "^12.4",
        "typo3/cms-fluid-styled-content": "^12.4",
        "typo3/cms-form": "^12.4",
        "typo3/cms-frontend": "^12.4",
        "typo3/cms-impexp": "^12.4",
        "typo3/cms-info": "^12.4",
        "typo3/cms-install": "^12.4",
        "typo3/cms-linkvalidator": "^12.4",
        "typo3/cms-lowlevel": "^12.4",
        "typo3/cms-recordlist": "^12.4",
        "typo3/cms-recycler": "^12.4",
        "typo3/cms-redirects": "^12.4",
        "typo3/cms-reports": "^12.4",
        "typo3/cms-rte-ckeditor": "^12.4",
        "typo3/cms-seo": "^12.4",
        "typo3/cms-setup": "^12.4",
        "typo3/cms-sys-note": "^12.4",
        "typo3/cms-t3editor": "^12.4",
        "typo3/cms-tstemplate": "^12.4",
        "typo3/cms-viewpage": "^12.4",
        "wapplersystems/ws-slider": "^12.1",
        "web-vision/wv_deepltranslate": "^4.5"
    },
    "scripts": {
        "typo3-cms-scripts": [
            "vendor/bin/typo3 install:fixfolderstructure"
        ],
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ]
    }
}
