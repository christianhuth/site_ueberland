## TitleTag [Begin]
config.noPageTitle = 2
page.headerData.1 = TEXT
page.headerData.1.field = title
page.headerData.1.htmlSpecialChars = 1
page.headerData.1.noTrimWrap = |<title>überland Tecklenburg &#124; | </title>|
[request && traverse(request.getQueryParams(), 'tx_ueberland_ueberland/angebot') > 0]
temp.angebotTitle = RECORDS
temp.angebotTitle {
  source = {GP:tx_ueberland_ueberland|angebot}
  source.insertData = 1
  tables = tx_ueberland_domain_model_angebot
  conf.tx_ueberland_domain_model_angebot >
  conf.tx_ueberland_domain_model_angebot = TEXT
  conf.tx_ueberland_domain_model_angebot.field = title
  wrap = <title>überland Tecklenburg &#124;&nbsp;|</title>
  htmlSpecialChars = 1
}
page.headerData.1 >
page.headerData.1 < temp.angebotTitle
[global]
## TitleTag [End]
