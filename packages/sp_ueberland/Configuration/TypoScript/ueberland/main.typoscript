config.no_cache = 0
config.doctype = html5
config.spamProtectEmailAddresses = 2

page = PAGE
page.typeNum = 0

page.10 = FLUIDTEMPLATE
page.10 {
  
  layoutRootPaths {
    0 = EXT:sp_ueberland/Resources/Private/Layouts
    1 = {$page.fluidtemplate.layoutRootPath}
  }
  partialRootPaths {
    0 = EXT:sp_ueberland/Resources/Private/Partials/
    1 = {$page.fluidtemplate.partialRootPath}
  }
  templateRootPaths {
    0 = EXT:sp_ueberland/Resources/Private/Templates
    1 = {$page.fluidtemplate.templateRootPath}
  }
  
  dataProcessing {
    # Page Contents
    10 = TYPO3\CMS\Frontend\DataProcessing\DatabaseQueryProcessor
    10 {
        table = tt_content
        orderBy = sorting
        where = colPos = 0
        as = content_main
    }
    11 = TYPO3\CMS\Frontend\DataProcessing\DatabaseQueryProcessor
    11 {
        table = tt_content
        orderBy = sorting
        where = colPos = 1
        as = content_slider
    }
    12 = TYPO3\CMS\Frontend\DataProcessing\DatabaseQueryProcessor
    12 {
        table = tt_content
        orderBy = sorting
        where = colPos = 2
        as = content_main2
    }
    # Navigation
    20 = language-menu
    20 {
        languages = auto
        as = languageNavigation
    }
  }
}

## Backend Layouts
page.10.file.stdWrap.cObject = CASE
page.10.file.stdWrap.cObject {
    
    key.data = levelfield:-1, backend_layout_next_level, slide
    key.override.field = backend_layout
    
    default = TEXT
    default.value = EXT:sp_ueberland/Resources/Private/Templates/Page/Default.html
    
    6 = TEXT
    6.value = EXT:sp_ueberland/Resources/Private/Templates/genussreisen-startseite.html
    
    7 = TEXT
    7.value = EXT:sp_ueberland/Resources/Private/Templates/genussreisen.html
    
    8 = TEXT
    8.value = EXT:sp_ueberland/Resources/Private/Templates/genussreisen-region.html

}

## CSS
page.includeCSS {
    file10 = EXT:sp_ueberland/Resources/Public/Css/ueberland.css
}

## Meta Tags
page {
  meta {
    author {
    	data = levelfield:-1, author, slide
    	field = author
    }
    description {
    	data = levelfield:-1, description, slide
    	field = description
    }
    keywords {
    	data = levelfield:-1, keywords, slide
    	field = keywords
    }
    summary {
    	data = levelfield:-1, summary, slide
    	field = summary
    }
    viewport = width=device-width, initial-scale=1
    X-UA-Compatible = IE=edge
    X-UA-Compatible.httpEquivalent = 1
  }
}

